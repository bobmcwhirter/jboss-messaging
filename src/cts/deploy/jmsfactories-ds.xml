<?xml version="1.0" encoding="UTF-8"?>

<connection-factories>

   <mbean code="org.jboss.mq.il.uil2.UILServerILService"
          name="jboss.mq:service=InvocationLayer,type=TopicConnectionFactory">
      <depends optional-attribute-name="Invoker">jboss.mq:service=Invoker</depends>
      <attribute name="ConnectionFactoryJNDIRef">TopicConnectionFactory</attribute>
      <attribute name="XAConnectionFactoryJNDIRef">TopicXAConnectionFactory</attribute>
      <attribute name="EnableTcpNoDelay">true</attribute>
   </mbean>
   <tx-connection-factory>
      <jndi-name>TopicConnectionFactory</jndi-name>
      <rar-name>jms-ra.rar</rar-name>
      <connection-definition>org.jboss.resource.adapter.jms.JmsConnectionFactory</connection-definition>
      <xa-transaction/>
      <config-property name="JmsProviderAdapterJNDI" type="java.lang.String">java:/DefaultJMSProvider</config-property>
      <config-property name="SessionDefaultType" type="java.lang.String">javax.jms.Topic</config-property>
      <application-managed-security/>
   </tx-connection-factory>
   <mbean code="org.jboss.naming.LinkRefPairService"
          name="jboss.jmx:alias=jms/TopicConnectionFactory">
      <attribute name="JndiName">jms/TopicConnectionFactory</attribute>
      <attribute name="RemoteJndiName">TopicConnectionFactory</attribute>
      <attribute name="LocalJndiName">java:/TopicConnectionFactory</attribute>
   </mbean>

